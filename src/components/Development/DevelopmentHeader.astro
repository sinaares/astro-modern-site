---
// components/SectionHeader.astro
import "../../styles/Development/DevelopmentHeader/StyleSheet.css";
const { title, highlight, description } = Astro.props;
---

<div class="header">
  <h2 class="title">
    {title}
    <span class="highlight">{highlight}</span>
  </h2>
  <p class="subtitle">
    {description}
  </p>
</div>
